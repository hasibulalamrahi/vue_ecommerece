id:product.id}}\">{{product.title}}</router-link>\n            </h4>\n            <h6 class=\"price\"><strong>${{product.price}}</strong></h6>\n            <!-- <p class=\"card_description\"><a href=\"#\" class=\"description\">{{product.description}}</a></p> <br> -->\n            <p class=\"card_description\"><router-link class=\"description\" :to=\"{name:'product',params:{id:product.id}}\">{{product.description}}</router-link>\n</p> <br>\n\n            <p class=\"card-text\">Rating:{{product.rating.rate}}</p>\n          </div>\n          \n          <!-- </router-link> -->\n          <div class=\"px-4 pb-3\">\n            <button class=\"btn btn-success\" id=\"cart\">Buy Now</button>\n            <button class=\"btn btn-warning\" id=\"cart\" @click=\"addToCart()\">Add to cart</button>\n          </div>\n        </div>\n      </div>\n  \n</template>\n\n<script>\nexport default {\n  props:[\"product\"],\n  methods:{\n    addToCart(){\n      this.$store.dispatch('addProductToCart',{\n         product:this.product,\n         quantity:1\n      })   \n    },\n    toProductLink(id){\n      // alert(id)\n     this.$router.push(`/product/${id}`)\n    }\n  }\n} \n</script>\n\n<style scoped>\n  /* .product__image {\n  margin-right: 1rem;\n} */\n.title{\n  font-size: 15px;\n  line-height:0;\n  text-decoration: none;\n}\n.product__image  {\n  /* margin-right: 1rem; */\n  padding: 10px;\n  height: 15rem;\n  width: 15rem;\n  object-fit: cover;\n  background-repeat: no-repeat;\n  /* justify-content: center; */\n  background-position: center center center;\n  /* object-position: center center; */\n  \n}\n\n.product__image:hover{\n  transform: scale(1.3);\n  z-index: 2;\n}\n\n.price{\n  text-align: center;\n  font-size:25px;\n  color: red;\n\n}\n.card-text{\n  padding: 0px;\n  margin-top: 0.5px;\n  text-align: center;\n  font-size: 14px;\n  font-weight: bold;\n\n}\n\n.card_description .description{\n  text-decoration: none;\n  color:black;\n\n}\n\n.card_description{\n  text-decoration: none;\n  color:black;\n  overflow: hidden;\n  text-overflow: ellipsis;\n  display: -webkit-box;\n  -webkit-line-clamp: 3;\n  -webkit-box-orient: vertical;\n}\n#cart{\n  width: 100%;\n  height: 40px;\n  font-size: 18px;\n  cursor: pointer;\n  margin-top: 10px;\n  border: none;\n  outline: none;\n}\n</style>"],"sourceRoot":""}]}