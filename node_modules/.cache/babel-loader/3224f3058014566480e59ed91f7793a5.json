n\">\n      <a class=\"nav-link dropdown-toggle\" href=\"#\" id=\"navbarDropdown\" role=\"button\" data-bs-toggle=\"dropdown\" aria-expanded=\"false\">\n            {{cartItemCount}}  <i class=\"fas fa-cart-plus\" id=\"cart\"></i>\n       </a>\n          \n      <div class=\"dropdown-menu p-2\" @click=\"$event.stopPropagation()\"\n                style=\"min-width:320px; right:0; left:auto\" \n                aria-labelledby=\"triggerId\">\n                <div>\n                  <div v-for=\"item in cart\" :key=\"item.product.id\">\n                    <div class=\"px-2 d-flex justify-content-between\">\n                        <div>\n                            <strong>\n                                {{item.product.title }}\n                            </strong>\n                            <br/> {{item.quantity}}x${{item.product.price}}\n                        </div>\n                        <div>\n                            <a href=\"#\" class=\"badge badge-secondary\" @click.prevent=\"removeProductFromCart(item.product)\">remove</a>\n                        </div>\n\n                    </div>\n                </div>\n                <hr/>\n                <div class=\"d-flex justify-content-between\">\n                    <span>Total: ${{cartTotalPrice}}</span>\n                    <a href=\"#\" @click.prevent=\"clearCartItems()\">Clear Cart</a>\n                </div> <br>\n                <div>\n                  <button type=\"button\" class=\"btn btn-info\" id=\"checkout\"><router-link to=\"/cart\" class=\"nav-tem nav-link active\">Checkout</router-link> </button>\n                </div>\n      \n    </div>\n    </div>\n        </li>\n       </div>\n</template>\n\n<script>\nconst carts = 'shopping-cart';\n\nexport default {\n    created(){\n      this.$store.state.cart=  JSON.parse(localStorage.getItem(carts)|| '[]');\n    },\n    computed:{\n      cart(){\n        return this.$store.state.cart;\n      },\n      cartItemCount(){\n        return this.$store.getters.cartItemCount;\n      },\n      cartTotalPrice(){\n         return this.$store.getters.cartTotalPrice;\n      }\n    },\n    methods:{\n      removeProductFromCart(product){\n         this.$store.dispatch(\"removeProductFromCart\",product);\n        //  localStorage.setItem(cart,JSON.stringify(this.$store.state.cart))\n      },\n      clearCartItems(){\n        this.$store.dispatch(\"clearCartItems\");\n      }\n    },\n\n    // mounted(){\n    //   this.$store.dispatch('getCartItems')\n    // }\n\n    \n}\n</script>\n\n<style>\n    #checkout{\n  width: 100%;\n  height: 40px;\n  font-size: 18px;\n  cursor: pointer;\n  margin-top: 10px;\n  border: none;\n  outline: none;\n}\n\n#cart{\n  color:white;\n  margin-right: 20px;\n  margin-top:10px;\n}\n</style>"],"sourceRoot":""}]}