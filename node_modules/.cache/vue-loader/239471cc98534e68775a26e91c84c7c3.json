s.$store.dispatch('addProductToCart',{\n         product:this.product,\n         quantity:1\n      })   \n    }\n  }\n} \n</script>\n\n<style scoped>\n  /* .product__image {\n  margin-right: 1rem;\n} */\n.title{\n  font-size: 20px;\n  text-decoration: none;\n}\n.product__image  {\n  /* margin-right: 1rem; */\n  padding: 10px;\n  height: 15rem;\n  width: 15rem;\n  object-fit: cover;\n  background-repeat: no-repeat;\n  /* justify-content: center; */\n  background-position: center center center;\n  /* object-position: center center; */\n  \n}\n\n.product__image:hover{\n  transform: scale(1.3);\n  z-index: 2;\n}\n\n.price{\n  text-align: center;\n  font-size:25px;\n  color: red;\n\n}\n.card-text{\n  padding: 0px;\n  margin-top: 0.5px;\n  text-align: center;\n  font-size: 14px;\n  font-weight: bold;\n\n}\n\n.card_description{\n  overflow: hidden;\n  text-overflow: ellipsis;\n  display: -webkit-box;\n  -webkit-line-clamp: 3;\n  -webkit-box-orient: vertical;\n}\n#cart{\n  width: 100%;\n  height: 40px;\n  font-size: 18px;\n  cursor: pointer;\n  margin-top: 10px;\n  border: none;\n  outline: none;\n}\n</style>"]}]}