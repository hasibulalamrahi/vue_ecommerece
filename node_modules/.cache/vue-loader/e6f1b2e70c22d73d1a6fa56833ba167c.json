duct\",this.id);\n    },\n\n    methods:{\n      addToCart(){\n        this.$store.dispatch(\"addProductToCart\",{\n          product:this.product,\n          quantity:1\n        })\n        localStorage.setItem(carts,JSON.stringify(this.$store.state.cart));\n      }\n    }\n}\n</script>\n\n<style>\n  .w-100:hover{\n    transform: scale(1.5);\n    z-index: 3;\n  }\n</style>"]}]}